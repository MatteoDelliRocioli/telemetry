{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppSettings": {
    "FlushOnWrite": false,
    "ShowNestedFlow": true,
    "AppInsightsKey": "",
    "TraceMessageFormatPrefix": "[{now}] {source} {category} {tidpid} - {sourceLevel} - {lastLineDeltaPadded} {deltaPadded} {nesting} {messageNesting}",
    "TraceMessageFormat": "[{now}] {source} {category} {tidpid} - {sourceLevel} - {lastLineDeltaPadded} {deltaPadded} {nesting} {messageNesting}{message}",
    "TraceMessageFormatStart": "[{now}] {source} {category} {tidpid} - {sourceLevel} - {lastLineDeltaPadded} {deltaPadded} {nesting} {messageNesting}{message}",
    "TraceMessageFormatStop": "[{now}] {source} {category} {tidpid} - {sourceLevel} - {lastLineDeltaPadded} {deltaPadded} {nesting} {messageNesting}{message}{result}"

  },
  //"ApiUrl": "https://botrecording-app.azurewebsites.net/api/v1/",
  "ApiUrl": "https://localhost:44381/",
  //"ApiUrl": "https://dev-samples-101-dc3w64-app.azurewebsites.net/api/v1/",
  "AllowedHosts": "*"
}
